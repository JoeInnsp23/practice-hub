  "calculationEngine": {
    "description": "Pricing calculation workflow",
    "steps": [
      {
        "step": 1,
        "action": "mapInputsToDrivers",
        "description": "Map lead/proposal fields to pricing drivers using 22-mappings.json",
        "output": "Structured pricing driver object"
      },
      {
        "step": 2,
        "action": "selectServices",
        "description": "Auto-select services based on lead data (if autoServiceConfig enabled)",
        "output": "Array of service IDs with configuration"
      },
      {
        "step": 3,
        "action": "calculateBasePrice",
        "description": "For each service, calculate base price using appropriate model",
        "models": {
          "modelA": "Lookup turnover band, apply complexity/industry multipliers",
          "modelB": "Calculate from transaction count, apply lower multipliers"
        },
        "output": "Base prices for Model A and Model B (where applicable)"
      },
      {
        "step": 4,
        "action": "applyModifiers",
        "description": "Apply complexity, industry, frequency, and time-based modifiers",
        "output": "Modified prices"
      },
      {
        "step": 5,
        "action": "addSurcharges",
        "description": "Add multi-currency, multi-entity, or other surcharges",
        "output": "Prices with surcharges"
      },
      {
        "step": 6,
        "action": "applyDiscounts",
        "description": "Apply volume, new client, annual payment, or custom discounts",
        "output": "Discounted prices"
      },
      {
        "step": 7,
        "action": "roundPrices",
        "description": "Apply rounding rules (if roundingToNearest5 enabled)",
        "output": "Final rounded prices"
      },
      {
        "step": 8,
        "action": "compareModels",
        "description": "Compare Model A vs Model B, recommend best option",
